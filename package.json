{
  "name": "Receipts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:adb": "adb reverse tcp:8081 tcp:8081",
    "ios": "npx react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "react-native start",
    "test": "jest",
    "reinstall-dependencies": "rm -rf node_modules/ && rm -rf yarn.lock && yarn install",
    "cocoapods": "cd ios && pod deintegrate && pod clean && pod install && cd ../",
    "watchman": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && rm -rf yarn.lock && yarn install && yarn start --reset-cache",
    "clearDerivedData": "rm -rf ~/Library/Developer/Xcode/DerivedData"
  },
  "dependencies": {
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-plugin-import": "2.29.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-dotenv": "3.4.11",
    "react-native-linear-gradient": "2.8.3",
    "react-native-pager-view": "6.3.0",
    "react-native-reanimated": "3.8.1",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.30.1",
    "react-native-tab-view": "3.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@babel/preset-env": "7.24.4",
    "@babel/runtime": "7.20.0",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native/metro-config": "0.75.0-main",
    "@tsconfig/react-native": "3.0.5",
    "@types/react": "18.2.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "babel-jest": "29.6.3",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.19.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.6.3",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "engines": {
    "node": ">=18"
  }
}
